    "version": "1.0.0",
    "publisher": "technoven",
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "ruby",
        "snippets"
    ],
    "homepage": "https://github.com/TechnoVen/ruby-snippet",
    "repository": {
        "type": "git",
